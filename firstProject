class classroom {
    int number;
    student user;
    
    public classroom(int _number, student _user){
        number = _number;
        user = _user;
    }
}

class student {
    String name;
    int age;
    
    public student(String _name, int _age){
        name = _name;
        age = _age;
    }
}

class main {
    public static void main(String[] args) {
        student student1 = new student("Anna", 21);
        classroom station1 = new classroom(1, student1);
        System.out.println(station1.user.name);
    }
}
